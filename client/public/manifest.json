{
  "manifest_version": 3,
  "name": "Contest Notifier",
  "version": "1.0",
  "permissions": [ "storage",
    "alarms",
    "windows",
    "notifications",
    "tabs"],
  "host_permissions": ["http://localhost:3000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["reminder.html"],
    "matches": ["<all_urls>"]
  }]
}
