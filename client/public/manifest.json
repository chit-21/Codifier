{
  "manifest_version": 3,
  "name": "Contest Notifier",
  "version": "1.0",
  "permissions": ["storage"],
  "host_permissions": ["http://localhost:3000/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": { "128": "vite.svg" }
}